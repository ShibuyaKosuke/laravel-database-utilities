{
    "name": "shibuyakosuke/laravel-database-utilities",
    "description": "database utilities for laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ShibuyaKosuke",
            "email": "shibuya@imagenious.co.jp"
        }
    ],
    "autoload": {
        "psr-4": {
            "ShibuyaKosuke\\LaravelDatabaseUtilities\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Table": "ShibuyaKosuke\\LaravelDatabaseUtilities\\Models\\Table",
                "Column": "ShibuyaKosuke\\LaravelDatabaseUtilities\\Models\\Column"
            }
        }
    },
    "require": {
        "diplodocker/comments-loader": "^0.0.2",
        "laravel/framework": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    }
}
